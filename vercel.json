{
    "version": 2,
    "builds": [
      {
        "src": "api/websocket.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/public/index.html"
      },
      {
        "src": "/errors.txt",
        "dest": "/public/errors.txt"
      },
      {
        "src": "/interactions.txt",
        "dest": "/public/interactions.txt"
      }
    ]
  }
  